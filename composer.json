{
    "name": "yiiplus/yii2-desktop",
    "description": "RBAC Auth manager for Yii2",
    "keywords": [
        "yii",
        "admin",
        "desktop",
        "auth",
        "rbac"
    ],
    "homepage": "https://github.com/yiiplus/",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiiplus/yii2-desktop/issues",
        "source": "https://github.com/yiiplus/yii2-desktop"
    },
    "authors": [
        {
            "name": "Hongbin Chen",
            "email": "hongbin.chen@aliyun.com"
        },
        {
            "name": "xiankun Geng",
            "email": "ge2x3k2@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "^2.0.0",
        "almasaeed2010/adminlte": "^2.4.0",
        "wenzhixin/bootstrap-table": "^1.0.0",
        "bower-asset/toastr": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-fileinput": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0.0",
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
    },
    "autoload": {
        "psr-4": {
            "yiiplus\\desktop\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}